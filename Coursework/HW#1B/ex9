x = magic(5)

x =

    17    24     1     8    15
    23     5     7    14    16
     4     6    13    20    22
    10    12    19    21     3
    11    18    25     2     9

sum(x)

ans =

    65    65    65    65    65

sum(x,2)

ans =

    65
    65
    65
    65
    65

sum(diag(x))

ans =

    65

sum(diag(flipud(x)))

ans =

    65
